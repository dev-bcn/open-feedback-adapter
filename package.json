{
  "name": "open-feedback-adapter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@next/env": "^16.1.6",
    "@tailwindcss/postcss": "^4.1.18",
    "axios": "1.13.3",
    "next": "16.1.6",
    "react": "^18",
    "react-dom": "^18",
    "test": "3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "jest": "^30.2.0",
    "postcss": "^8",
    "tailwindcss": "^4.1.16",
    "ts-jest": "^29.4.6",
    "typescript": "^5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "<rootDir>/src/jest.setup.js"
    ]
  }
}
